    "import torch\n",
    "from archisound import ArchiSound\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "outputs": [],
   "source": [
    "autoencoder = ArchiSound.from_pretrained('autoencoder1d-AT-v1')\n",
    "\n",
    "x = torch.randn(1, 2, 2**18)    # [1, 2, 262144]\n",
    "z = autoencoder.encode(x)       # [1, 32, 8192]\n",
    "y = autoencoder.decode(z)       # [1, 2, 262144]"
   ],
